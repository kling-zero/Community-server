<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zeroone.star.scmenu.mapper.RoleMapper">

    <select id="getOneByName" resultType="com.zeroone.star.scmenu.entity.Role">
        select id, name, keyword, description
        from
            role
        where
            name = #{roleName}
    </select>
</mapper>
